{
    "name": "health-backend",
    "version": "5.1.1",
    "description": "Health application backend",
    "author": "technology@health.com <Health Technology>",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "migration:up": "npx db-migrate up",
        "migration:down": "npx db-migrate down",
        "migration:create": "npx db-migrate create"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.45.0",
        "@aws-sdk/client-sqs": "^3.53.0",
        "@google-cloud/storage": "^6.8.0",
        "@nestjs/common": "^8.0.0",
        "@nestjs/config": "^1.0.1",
        "@nestjs/core": "^8.0.0",
        "@nestjs/mongoose": "^9.2.2",
        "@nestjs/platform-express": "^8.0.0",
        "@nestjs/swagger": "^5.0.9",
        "@nestjs/typeorm": "^8.0.2",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/lodash": "^4.14.175",
        "@types/multer": "^1.4.7",
        "aws-cloudfront-sign": "^2.2.0",
        "aws-sdk": "^2.1109.0",
        "axios": "^0.23.0",
        "bcryptjs": "^2.4.3",
        "cheerio": "^1.0.0-rc.10",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.1",
        "cookie-parser": "^1.4.5",
        "date-fns": "^2.28.0",
        "date-fns-tz": "^1.3.4",
        "db-migrate": "^0.11.12",
        "db-migrate-pg": "^1.2.2",
        "dotenv": "^10.0.0",
        "dotenv-extended": "^2.9.0",
        "google-auth-library": "^7.9.2",
        "jsonwebtoken": "^8.5.1",
        "lodash": "4.17.21",
        "mongoose": "^7.0.3",
        "morgan": "^1.10.0",
        "multer-google-storage": "^1.3.0",
        "oauth2-server": "^3.1.1",
        "pg": "^8.7.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "soft-delete-plugin-mongoose": "^1.0.15",
        "swagger-ui-express": "^4.1.6",
        "typeorm": "^0.2.37",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.0.0",
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/testing": "^8.0.0",
        "@types/bcrypt": "^5.0.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.0.0",
        "@types/oauth2-server": "^3.0.13",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^4.31.0",
        "@typescript-eslint/parser": "^4.28.2",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^27.0.6",
        "prettier": "^2.3.2",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.10.1",
        "typescript": "^4.3.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
